{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log\\DSLR-.log",
          "rollingInterval": "Hour"
        }
      }
    ]
  },
  "Configuration": {
    "DataSourceConfigs": [
      {
        "Name": "EquipParamWeapon",
        "SourceType": "RegulationBin",
        "Filters": [
          {
            "Field": "wepType",
            "Operator": "NotInRange",
            "Value": "81..86"
          },
          {
            "Field": "wepType",
            "Operator": "NotEqual",
            "Value": 33
          },
          {
            "Field": "wepmotionCategory",
            "Operator": "GreaterThan",
            "Value": 0
          },
          {
            "Field": "wepType",
            "Operator": "GreaterThan",
            "Value": 0
          },
          {
            "Field": "sortId",
            "Operator": "LessThan",
            "Value": 9999999
          },
          {
            "Field": "ID",
            "Operator": "EndsWith",
            "Value": "0000"
          }
        ]
      },
      {
        "Name": "EquipParamCustomWeapon",
        "SourceType": "RegulationBin"
      },
      {
        "Name": "EquipParamAccessory",
        "SourceType": "RegulationBin",
        "Filters": [
          {
            "Field": "ID",
            "Operator": "LessThan",
            "Value": 10000
          }
        ]
      },
      {
        "Name": "EquipParamProtector",
        "SourceType": "RegulationBin",
        "Filters": [
          {
            "Field": "sortGroupId",
